{
  "private": true,
  "scripts": {
    "codegen": "ts-node --project codegen/tsconfig.json --transpile-only codegen",
    "generate": "graphql-codegen",
    "clean": "rimraf build coverage",
    "start": "parcel serve src/index.html",
    "build": "parcel build src/index.html",
    "test": "jest --verbose",
    "precoverage": "rimraf coverage",
    "coverage": "yarn test --coverage",
    "format": "prettier --write src/{*,**/*}.{ts,tsx}",
    "lint": "yarn prettier --list-different src/{*,**/*}.{ts,tsx}",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@popmotion/popcorn": "^0.4.4",
    "antd": "^4.2.0",
    "array-move": "^2.2.1",
    "ethers": "^5.0.0-beta.186",
    "formik": "^2.1.4",
    "framer-motion": "^1.10.3",
    "graphql": "^15.0.0",
    "immer": "^9.0.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-use": "^14.2.0",
    "urql": "^1.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.9.6",
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/near-operation-file-preset": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-operations": "^1.13.5",
    "@graphql-codegen/typescript-urql": "^1.13.5",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "babel-plugin-import": "^1.13.0",
    "glob": "^7.1.6",
    "jest": "^25.4.0",
    "less": "^3.11.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-morph": "^7.0.3",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "rootDir": "src",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js"
    ],
    "coverageDirectory": "coverage"
  },
  "codegen": {
    "input": "../build"
  }
}
